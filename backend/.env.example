# Server Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Database (Supabase)
DATABASE_URL=postgresql://postgres:password@localhost:5432/rdcs_pricing
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Google Maps APIs
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
GOOGLE_GEOCODING_API_KEY=your-geocoding-api-key
GOOGLE_STREET_VIEW_API_KEY=your-street-view-api-key
GOOGLE_STATIC_MAPS_API_KEY=your-static-maps-api-key

# AI Vision (OpenAI GPT-4 Vision)
OPENAI_API_KEY=your-openai-api-key

# Alternative: Google Cloud Vision
# GOOGLE_CLOUD_PROJECT_ID=your-project-id
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# Property Data API (Regrid or Loveland)
REGRID_API_KEY=your-regrid-api-key
# LOVELAND_API_KEY=your-loveland-api-key

# Jobber API
JOBBER_API_KEY=your-jobber-api-key
JOBBER_API_SECRET=your-jobber-api-secret
JOBBER_ACCOUNT_ID=your-jobber-account-id
JOBBER_API_URL=https://api.getjobber.com/api/graphql

# RDC's Service Area
SERVICE_AREA_CENTER_LAT=39.1031
SERVICE_AREA_CENTER_LNG=-84.5120
SERVICE_AREA_RADIUS_MILES=25

# Rate Card Defaults
DEFAULT_COMPLEXITY_MULTIPLIER=1.0
DEFAULT_REGIONAL_MULTIPLIER=1.0
